{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": [
        "nvim-lspconfig",
        "lsp"
      ]
    }
  },
  "neoconf": {
    "plugins": {
      "sumneko_lua": {
        "enabled": true
      }
    }
